  <process id="Journalist_Invitation_Response" name="Journalist_Invitation_Response" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <serviceTask id="sendInvitationmail" name="Einladungsmail verschicken" activiti:class="ac.at.fhkufstein.activiti.delegates.SendInvitationmail"></serviceTask>
    <userTask id="defineVariables" name="Variablen definieren" activiti:assignee="System" activiti:formKey="none"></userTask>
    <sequenceFlow id="flow2" sourceRef="startevent1" targetRef="defineVariables"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="defineVariables" targetRef="sendInvitationmail"></sequenceFlow>
    <userTask id="answerInvitation" name="Einladung beantworten" activiti:formKey="answerInvitation.xhtml"></userTask>
    <sequenceFlow id="flow4" sourceRef="sendInvitationmail" targetRef="answerInvitation"></sequenceFlow>
    <boundaryEvent id="answerInvitationBoundarytimer" name="answerInvitationBoundarytimer" attachedToRef="answerInvitation" cancelActivity="true">
      <timerEventDefinition>
        <timeDate>${cancelInvitationTime}</timeDate>
      </timerEventDefinition>
    </boundaryEvent>
    <exclusiveGateway id="reminderSent" name="Reminder Sent"></exclusiveGateway>
    <sequenceFlow id="flow5" sourceRef="answerInvitationBoundarytimer" targetRef="reminderSent"></sequenceFlow>
    <sequenceFlow id="flow6" name="noch kein Urgenzmail versendet" sourceRef="reminderSent" targetRef="sendInvitationmail">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reminderSent == false}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="eventIsOpen" name="eventIsOpen"></exclusiveGateway>
    <sequenceFlow id="flow7" name="Urgenzmail bereits versendet" sourceRef="reminderSent" targetRef="eventIsOpen">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reminderSent == true}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="inviteNextParticipant" name="naechsten Teilnehmer nachladen" activiti:class="ac.at.fhkufstein.activiti.delegates.InviteNextParticipant"></serviceTask>
    <sequenceFlow id="flow8" name="Event offen" sourceRef="eventIsOpen" targetRef="inviteNextParticipant">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${eventIsOpen == true}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow9" sourceRef="inviteNextParticipant" targetRef="endevent1"></sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow10" name="Event geschlossen" sourceRef="eventIsOpen" targetRef="endevent2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${eventIsOpen == false}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="answer" name="answer"></exclusiveGateway>
    <sequenceFlow id="flow11" sourceRef="answerInvitation" targetRef="answer"></sequenceFlow>
    <sequenceFlow id="flow12" name="Zusage" sourceRef="answer" targetRef="typeOfJourney">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${invitationAccepted == true}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="kindOfRejection" name="kindOfRejection"></exclusiveGateway>
    <sequenceFlow id="flow14" name="Absage" sourceRef="answer" targetRef="kindOfRejection">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${invitationAccepted == false}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="selectiveInvitation" name="Selektive Nachladung"></userTask>
    <sequenceFlow id="flow15" name="normal" sourceRef="kindOfRejection" targetRef="eventIsOpen"></sequenceFlow>
    <sequenceFlow id="flow17" sourceRef="selectiveInvitation" targetRef="inviteSelectedParticipant"></sequenceFlow>
    <serviceTask id="inviteSelectedParticipant" name="ausgewaehlten Teilnehmer nachladen" activiti:class="ac.at.fhkufstein.activiti.delegates.InviteSelectedParticipant"></serviceTask>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow18" sourceRef="inviteSelectedParticipant" targetRef="endevent3"></sequenceFlow>
    <sequenceFlow id="flow19" name="Vertretung" sourceRef="kindOfRejection" targetRef="selectiveInvitation"></sequenceFlow>
    <exclusiveGateway id="typeOfJourney" name="Type of Journey"></exclusiveGateway>
    <sequenceFlow id="flight" name="Flug" sourceRef="typeOfJourney" targetRef="whichFlight"></sequenceFlow>
    <sequenceFlow id="car" name="Auto" sourceRef="typeOfJourney" targetRef="endevent4"></sequenceFlow>
    <endEvent id="endevent4" name="End"></endEvent>
    <exclusiveGateway id="whichFlight" name="whichFlight"></exclusiveGateway>
    <userTask id="manualExecution" name="manuelle Abwicklung"></userTask>
    <sequenceFlow id="specialFlight" name="Spezialflug" sourceRef="whichFlight" targetRef="manualExecution"></sequenceFlow>
    <sequenceFlow id="predefinedFlight" name="Standardflug" sourceRef="whichFlight" targetRef="sendBookingMail"></sequenceFlow>
    <serviceTask id="sendBookingMail" name="Buchungsmail senden" activiti:class="ac.at.fhkufstein.activiti.delegates.SendBookingMail"></serviceTask>
    <userTask id="supplyFlightInfos" name="Flugdaten eingeben"></userTask>
    <sequenceFlow id="flow21" sourceRef="sendBookingMail" targetRef="supplyFlightInfos"></sequenceFlow>
    <serviceTask id="sendTicketNotification" name="Ticketbenachrichtigung senden" activiti:class="ac.at.fhkufstein.activiti.delegates.SendTicketNotification"></serviceTask>
    <sequenceFlow id="flow22" sourceRef="supplyFlightInfos" targetRef="sendTicketNotification"></sequenceFlow>
    <endEvent id="endevent5" name="End"></endEvent>
    <sequenceFlow id="flow23" sourceRef="sendTicketNotification" targetRef="endevent5"></sequenceFlow>
    <endEvent id="endevent6" name="End"></endEvent>
    <sequenceFlow id="flow24" sourceRef="manualExecution" targetRef="endevent6"></sequenceFlow>
  </process>
